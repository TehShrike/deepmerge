{
	"compilerOptions": {
		"target": "ES2020",
		"module": "ES2020",
		"lib": [
			"ES2020"
		],
		"moduleResolution": "node",
		"esModuleInterop": true,
		"strict": true,
		"skipLibCheck": true,
		"forceConsistentCasingInFileNames": true,
		"removeComments": true,
		"baseUrl": ".",
		"paths": {
			"deepmerge": [
				"src/"
			]
		}
	}
}
